--

CREATE TABLE choices (
    id integer NOT NULL,
    user_id integer,
    num smallint,
    title character varying(100),
    comment character varying(2000),
    "timestamp" timestamp without time zone DEFAULT now(),
    expires date,
    is_active boolean DEFAULT true,
    photo1 integer,
    photo2 integer,
    item_type1 smallint DEFAULT 0 NOT NULL,
    item_type2 smallint DEFAULT 0 NOT NULL,
    gender1 boolean,
    gender2 boolean,
    item_id1 integer,
    item_id2 integer,
    good_name1 character varying(50),
    good_name2 character varying(50),
    shop_id1 integer,
    shop_id2 integer,
    brand_id1 integer,
    brand_id2 integer,
    price1 double precision,
    price2 double precision,
    visibility smallint DEFAULT 1 NOT NULL,
    num_votes1 integer DEFAULT 0 NOT NULL,
    num_votes2 integer DEFAULT 0 NOT NULL,
    num_votes_yes integer DEFAULT 0 NOT NULL,
    num_votes_no integer DEFAULT 0 NOT NULL,
    num_comments_users integer DEFAULT 0 NOT NULL,
    num_comments_pros integer DEFAULT 0 NOT NULL,
    num_comments_users_seen integer DEFAULT 0 NOT NULL,
    num_comments_pros_seen integer DEFAULT 0 NOT NULL
);


ALTER TABLE public.choices OWNER TO ylg;

