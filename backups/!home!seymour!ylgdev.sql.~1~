-- TOC entry 381 (class 1255 OID 60083)
-- Dependencies: 6
-- Name: field(anyelement, anyarray); Type: FUNCTION; Schema: public; Owner: ylg
--

CREATE FUNCTION field(anyelement, VARIADIC anyarray) RETURNS integer
    LANGUAGE sql STABLE
    AS $_$
  SELECT
    COALESCE(
     ( SELECT i FROM generate_subscripts($2, 1) gs(i)
       WHERE $2[i] = $1 ),
     0);
$_$;


ALTER FUNCTION public.field(anyelement, VARIADIC anyarray) OWNER TO ylg;

--
-- TOC entry 382 (class 1255 OID 60084)
